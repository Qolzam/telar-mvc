  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/chai-subset": "^1.3.1",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.43",
    "@types/supertest": "^2.0.3",
    "body-parser": "^1.18.2",
